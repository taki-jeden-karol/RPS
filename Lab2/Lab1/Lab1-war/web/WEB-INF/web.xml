<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
  version="4.0">
    
    <servlet>
        <servlet-name>ServletMasta</servlet-name>
        <servlet-class>servlets.ServletMasta</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servlet.ServletMasta</servlet-name>
        <url-pattern>/master</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletAdd</servlet-name>
        <servlet-class>servlets.ServletAdd</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletAdd</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ServletDelete</servlet-name>
        <servlet-class>servlets.ServletDelete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletDelete</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ServletEdit</servlet-name>
        <servlet-class>servlets.ServletEdit</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletEdit</servlet-name>
        <url-pattern>/edit</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ServletShow</servlet-name>
        <servlet-class>servlets.ServletShow</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletShow</servlet-name>
        <url-pattern>/show</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ServletCountAdd</servlet-name>
        <servlet-class>servlets.ServletCountAdd</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletCountAdd</servlet-name>
        <url-pattern>/addcount</url-pattern>
    </servlet-mapping>
</web-app>
